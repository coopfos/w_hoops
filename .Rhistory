library(moments)
# Define the markets (EURUSD is removed) and start date
markets <- c("CL=F", "GC=F", "HG=F", "BTC-USD",
"ES=F", "MNQ=F", "NG=F", "RTY=F", "SI=F", "MYM=F")
start_date <- Sys.Date() - 365
start_date <- Sys.Date() - 1460
# Download market data from Yahoo Finance
getSymbols(Symbols = markets, from = start_date, src = 'yahoo')
# Process each market symbol to add a DailyReturn column and rename the object
for (sym in markets) {
# Create a simplified symbol name by removing "=F" or "=X"
simple_sym <- gsub("=F|=X", "", sym)
# Get the data for the original symbol
data_xts <- get(sym)
# Calculate the daily percentage change: (Close - Open) / Open
daily_pct_change <- (Cl(data_xts) - Op(data_xts)) / Op(data_xts)
# Add the daily % change as a new column in the data
data_xts$DailyReturn <- daily_pct_change
# Save the modified data under the simplified symbol name
assign(simple_sym, data_xts)
}
# Define a threshold grid from -5% to 5% in increments of 0.01% (i.e. 0.0001)
thresholds <- seq(-0.05, 0.05, by = 0.0001)
# Initialize the output dataframe with the Threshold column
kde_df <- data.frame(Threshold = thresholds)
# For each instrument, estimate the KDE and compute tail probabilities
for (sym in markets) {
simple_sym <- gsub("=F|=X", "", sym)
# Retrieve the daily returns for this instrument; remove NAs
instrument_data <- get(simple_sym)
returns <- na.omit(instrument_data$DailyReturn)
# Compute the KDE over the fixed range; use the same number of grid points as thresholds
dens <- density(returns, from = -0.05, to = 0.05, n = length(thresholds))
# Approximate the cumulative distribution function (CDF) using the trapezoidal rule
dx <- diff(dens$x)
# Compute cumulative sum: start with 0 at the lower bound
cumdens <- c(0, cumsum((head(dens$y, -1) + tail(dens$y, -1)) / 2 * dx))
# Normalize so that the total area equals 1
total_area <- tail(cumdens, 1)
cdf <- cumdens / total_area
# Create an interpolation function for the CDF
Ffun <- approxfun(dens$x, cdf, yleft = 0, yright = 1)
# For each threshold, calculate the tail probability: P(DailyReturn > threshold)
tail_prob <- 1 - Ffun(thresholds)
# Add the tail probability as a new column in the results data frame
kde_df[[simple_sym]] <- tail_prob
}
# Display the first few rows of the resulting KDE probability dataframe
head(kde_df)
View(kde_df)
